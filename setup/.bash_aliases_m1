# Locations
alias m1="cd $HOME/Mock1"
alias m1c="cd $HOME/Mock1/m1_code"

# ROS
alias sm1="source $HOME/Mock1/m1_code/devel/setup.bash"
alias tinfo="rostopic info"
alias techo="rostopic echo"
alias tgrp="rostopic list | grep"
alias sgrp="rosservice list | grep"
alias ngrp="rosnode list | grep"
alias ninfo="rosnode info"

# Git
alias gta="git add"
alias gtch="git checkout"
alias gtcm="git commit -m"
alias gtd="git diff"
alias gtf="git fetch"
alias gtl="git log"
alias gtp="git pull"
alias gts="git status"

# Standard
alias b="cd .."
alias bb="cd ../.."
alias bbb="cd ../../.."
alias hgrep="history | grep"

# Navigation
alias = teleop_m1="rosrun teleop_twist_keyboard teleop_twist_keyboard.py _speed:=0.125 _turn:=0.25"